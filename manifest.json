{
	"name": "News Manager",
	"description": "A quick-way to browse headlines and stories from your favorite news sources without leaving what you're doing.",
	"version": "1.0.3",

	"permissions": [
		"activeTab",
		"storage",
		"http://fonts.googleapis.com/",
		"https://fonts.googleapis.com/"
	],

	"options_page": "settings.html",

	"browser_action": {
		"default_icon": "assets/icon48.png",
		"default_title": "News Manager",
		"default_popup": "feed.html"
	},
	
	"icons": {
			"128": "assets/icon128.png",
			"48": "assets/icon48.png"
	},

	"manifest_version": 2
}